{
    "FullRich": {
        "Serilog": {
            "Using": [ "Serilog.Sinks.GoogleCloudLogging" ],
            "MinimumLevel": {
                "Default": "Information",
                "Override": {
                    "Microsoft": "Warning",
                    "Microsoft.AspNetCore": "Warning",
                    "System.Net.Http.HttpClient": "Error"
                }
            },
            "WriteTo": [
                {
                    "Name": "Console",
                    "Args": {
                        "restrictedToMinimumLevel": "Error"
                    }
                },
                {
                    "Name": "GoogleCloudLogging",
                    "Args": {
                        "projectID": "wallet-dev-341406",
                        "useJsonOutput": "true",
                        "resourceType": "k8s_pod",
                        "resourceLabels": {
                            "project_id": "wallet-dev-341406",
                            "cluster_name": "wallet-dev",
                            "namespace_name": "h1",
                            "pod_name": "agentmobile"
                        },
                        "OutputTemplate": "{Timestamp:yyyy/MM/dd HH:mm:ss.fff zzz} {Application} [{Level}] {Message}{NewLine}{Exception}",
                        "restrictedToMinimumLevel": "Information"
                    }
                }
            ],
            "Properties": {
                "Application": "AgentMobile"
            }
        }
    }
}
